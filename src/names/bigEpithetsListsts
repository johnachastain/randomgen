// Epithets
//   the Lesser
//   the Greater
//   the Bald
//   the Impure
//   the Unready
//   the Grim
//   The Mad
//   the First, Second, Third, etc
//   the Miserable
//   the Fat
//   the Unreliable
//   the Forgotten
//   the Incompetent
//   the Slippery
//   the Inconsistent


export enum Epithets {
    Bestial = "Bestial",
    Bold = "Bold",
    Charming = "Charming",
    Devious = "Devious",
    Dextrous = "Dextrous",
    Epithet = "Epithet",
    Evil = "Evil",
    Fast = "Fast",
    Fierce = "Fierce",
    Good = "Good",
    Infirm = "Infirm",
    Noble = "Noble",
    Personality = "Personality",
    Physical = "Physical",
    Strong = "Strong",
    Tough = "Tough",
    Wise = "Wise"
}

const {
    Bestial,
    Bold,
    Charming,
    Devious,
